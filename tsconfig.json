{
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "built",
    "skipLibCheck": true,
    "sourceMap": true,          
    "strictNullChecks": true,   
    "module": "es6",            
    "jsx": "react",             
    "target": "es5",            
    "lib": ["es5", "es2017", "dom"],
    "allowJs": true,            
    "paths": {
      "*": ["*", "src/*"]
    }
  }
}